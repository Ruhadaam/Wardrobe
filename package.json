{
  "name": "wardrobe",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios --device",
    "ios:build": "cd ios && pod install && cd .. && xcodebuild -workspace ios/Wardrobe.xcworkspace -scheme Wardrobe -configuration Release -archivePath build/Wardrobe.xcarchive archive",
    "pod:install": "cd ios && pod install && cd ..",
    "web": "expo start --web",
    "deploy": "npx expo export -p web && npx eas-cli@latest deploy"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.2",
    "@google-cloud/vision": "^5.3.4",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@supabase/supabase-js": "^2.86.0",
    "base64-js": "^1.5.1",
    "date-fns": "^4.1.0",
    "expo": "~54.0.29",
    "expo-constants": "~18.0.8",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "~19.0.19",
    "expo-font": "^14.0.9",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linking": "~8.0.10",
    "expo-router": "~6.0.19",
    "expo-splash-screen": "~31.0.12",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "lottie-react-native": "^7.3.4",
    "nativewind": "^4.0.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-quick-crypto": "^0.7.17",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.0",
